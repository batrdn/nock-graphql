{
  "name": "nock-graphql",
  "version": "1.1.6",
  "description": "Nock-based GraphQL testing library",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/batrdn/nock-graphql.git",
  "author": "batrdn <baterdene.tsogoo220@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.*",
    "npm": ">=9.*"
  },
  "keywords": [
    "graphql test",
    "nock",
    "client-side graphql test",
    "unit testing",
    "graphql test mock"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier -u --check .",
    "format:fix": "prettier -u --write .",
    "lint": "eslint src --ext .js,.ts,",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:coverage": "npm run test --coverage --colors",
    "prepare": "husky install",
    "tsc:check": "tsc --noEmit",
    "prepublish": "npm run build",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "cross-fetch": ">= 4",
    "graphql": ">= 15",
    "nock": ">= 13"
  },
  "devDependencies": {
    "@apollo/client": "^3.9.5",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "semantic-release": "^23.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
